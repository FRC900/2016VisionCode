layer {
  name: "val-data"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TEST
  }
  transform_param {
    scale: 0.00390625
    mirror: true
    mean_value: 127.0
  }
  data_param {
    source: "/home/kjaget/DIGITS/digits/jobs/20160712-072502-9f12/val_db"
    batch_size: 128
    backend: LMDB
